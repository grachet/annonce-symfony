    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="guillaume rachet">
    <title>{% block title %}Annonce{% endblock %}</title>

    {% block stylesheetsbootstrap %}
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

        <link rel="stylesheet" href="{{ asset('bundles/occore/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/occore/css/mdb.min.css') }}">
